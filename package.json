{
  "name": "@btwiuse/ts-node-shebang",
  "description": "distribute executable TypeScript snippets on npmjs.org without compilation to JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/btwiuse/ts-node-shebang.git"
  },
  "version": "0.1.0",
  "bin": {
    "btwiuse-ts-node-shebang": "./index.ts"
  },
  "scripts": {
    "format": "npx pretty-quick",
    "publish": "yarn npm publish --access public"
  },
  "files": [
    "*.ts",
    "tsconfig.json"
  ],
  "license": "Unlicense",
  "dependencies": {
    "commander": "^8.1.0",
    "figlet-promised": "^1.0.0",
    "fs": "^0.0.1-security",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^16.4.6",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": {
    "name": "btwiuse"
  }
}
